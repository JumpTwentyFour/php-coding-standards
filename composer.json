{
    "name": "jumptwentyfour/php-coding-standards",
    "description": "Coding standards for our PHP applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Williams",
            "email": "alex@jump24.co.uk",
            "homepage": "https://jump24.co.uk",
            "role": "Lead Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "JumpTwentyFour\\PhpCodingStandards\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "JumpTwentyFour\\PhpCodingStandards\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "slevomat/coding-standard": "^6.4",
        "symplify/easy-coding-standard": "^9.1",
        "nunomaduro/larastan": "^0.7",
        "nikic/php-parser": "^4.0",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "phpstan/phpstan-strict-rules": "^0.12.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3 || ^8.2 || ^9.3"
    }
}
